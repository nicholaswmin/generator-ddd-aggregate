{
  "name": "generator-ddd-aggregate",
  "version": "0.1.1",
  "description": "Generate Domain Driven Design Aggregates for Node.js",
  "homepage": "https://github.com/nicholaswmin/generator-ddd-aggregate#readme",
  "author": "@nicholaswmin",
  "files": ["generators"],
  "main": "index.js",
  "keywords": [
    "domain-driven-design",
    "aggregate",
    "node.js",
    "yeoman-generator"
  ],
  "devDependencies": {
    "yeoman-test": "^1.7.0",
    "yeoman-assert": "^3.1.0",
    "nsp": "^2.6.3",
    "eslint": "^4.1.0",
    "prettier": "^1.7.0",
    "husky": "^0.14.3",
    "lint-staged": "^4.2.1",
    "eslint-config-prettier": "^2.4.0",
    "eslint-plugin-prettier": "^2.2.0",
    "eslint-config-xo": "^0.18.2",
    "jest": "^21.0.0",
    "jest-cli": "^21.0.0"
  },
  "scripts": {
    "test": "jest test",
    "prepublish": "nsp check",
    "pretest": "eslint .",
    "precommit": "lint-staged"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nicholaswmin/generator-ddd-aggregate.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nicholaswmin/generator-ddd-aggregate/issues"
  },
  "dependencies": {
    "yeoman-generator": "^2.0.1",
    "chalk": "^2.1.0",
    "yosay": "^2.0.1"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.js": ["eslint --fix", "git add"],
    "*.json": ["prettier --write", "git add"]
  },
  "eslintConfig": {
    "extends": ["xo", "prettier"],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "printWidth": 90
        }
      ]
    },
    "plugins": ["prettier"]
  }
}
